<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="panda_pba_robots">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import panda_pba_robots urdf file -->
    <xacro:include filename="$(find sirgas_description)/urdf/panda_pba.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="panda_pba_robots.ros2_control.xacro" />


    <xacro:panda_pba_robots_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
