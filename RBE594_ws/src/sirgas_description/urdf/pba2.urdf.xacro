<?xml version="1.0" ?>
<robot name="peg_board_assembly" xmlns:xacro="http://wiki.ros.org/xacro" >
    <xacro:include filename="$(find sirgas_description)/urdf/inc/apriltag_card.xacro"/>
    <joint name="pb_world_joint" type="fixed">
        <parent link="world"/>
        <child link="peg_board"/>
        <origin rpy="0 0 0" xyz="0 0 0"/>
    </joint>

    <link name="peg_board">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Peg_Board.stl" />
            </geometry>
            <material name="LightGrey">
                <color rgba="0.7 0.7 0.7 1.0" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Peg_Board.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="170.447"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="3.75895934" ixy="0.0" ixz="0.0" iyx="0.0" iyy="14.51416559" iyz="0.0" izx="-0.02634141" izy= "0.0" izz="17.74821744"/>
        </inertial>
    </link>

    <joint name="pb_starter_joint" type="continuous">
        <parent link="peg_board"/>
        <child link="starter_gear"/>
        <origin rpy="0 0 0" xyz="-0.375 0 0.125"/>
        <axis xyz="0 0 1"/>
        <limit effort="1500.0" velocity="10.0"/>
        <dynamics damping="0.1" friction="0.01"/>
    </joint>

    <link name="starter_gear">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Starter_Gear.stl" />
            </geometry>
            <material name="LightGrey">
                <color rgba="0.7 0.7 0.7 1.0" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Starter_Gear.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="4.903"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="0.0731301" ixy="0.0" ixz="0.0" iyx="0.0" iyy="0.073" iyz="0.0" izx="0.0" izy= "0.0" izz="0.14570353"/>
        </inertial>
    </link>

    <joint name="pb_first_joint" type="continuous">
        <parent link="peg_board"/>
        <child link="first_gear"/>
        <origin rpy="0 0 0" xyz="0 0 0.125"/>
        <axis xyz="0 0 1"/>
        <limit effort="1500.0" velocity="10.0"/>
        <dynamics damping="0.1" friction="0.01"/>
        
        <mimic joint="pb_starter_joint" multiplier="-1.76923" offset="0"/>
    </joint>

    <link name="first_gear">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/First_Gear.stl" />
            </geometry>
            <material name="LightGrey">
                <color rgba="0.7 0.7 0.7 1.0" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/First_Gear.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="1.409"/>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <inertia ixx="0.00615061" ixy="0.0" ixz="0.0" iyx="0.0" iyy="0.00615163" iyz="0.0" izx="0.0" izy= "0.0" izz="0.01209542"/>
        </inertial>
    </link>
    <xacro:attach_apriltag_card
        parent_link="first_gear"
        tag_size=".5"                
        tag_thickness="0.00001"           
        xyz="0 .06 .02"                  
        rpy="0 3.0 0"                     
        link_name="first_gear_apriltag"
    />
    <joint name="pb_second_joint" type="continuous">
        <parent link="peg_board"/>
        <child link="second_gear"/>
        <origin rpy="0 0 0" xyz="0.25 0 0.125"/>
        <axis xyz="0 0 1"/>
        <limit effort="1500.0" velocity="10.0"/>
        <dynamics damping="0.1" friction="0.01"/>
        
        <mimic joint="pb_first_joint" multiplier="-1.0" offset="0"/>
    </joint>

    <link name="second_gear">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Second_Gear.stl" />
            </geometry>
            <material name="LightGrey">
                <color rgba="0.7 0.7 0.7 1.0" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Second_Gear.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="1.141"/>
            <origin rpy="0 0 0" xyz="0 0 0.01359774"/>
            <inertia ixx="0.00406128" ixy="0.0" ixz="0.0" iyx="0.0" iyy="0.00406281" iyz="0.0" izx="0.0" izy= "0.0" izz="0.00794573"/>
        </inertial>
    </link>

    <joint name="pb_third_joint" type="continuous">
        <parent link="peg_board"/>
        <child link="third_gear"/>
        <origin rpy="0 0 0" xyz="0.45 0 0.125"/>
        <axis xyz="0 0 1"/>
        <limit effort="1500.0" velocity="10.0"/>
        <dynamics damping="0.1" friction="0.01"/>
        
        <mimic joint="pb_second_joint" multiplier="-1.625" offset="0"/>
    </joint>

    <link name="third_gear">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Third_Gear.stl" />
            </geometry>
            <material name="LightGrey">
                <color rgba="0.7 0.7 0.7 1.0" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="file://$(find sirgas_description)/meshes/Third_Gear.stl" />
            </geometry>
        </collision>
        <inertial>
            <mass value="0.538"/>
            <origin rpy="0 0 0" xyz="0 0 0.01483255"/>
            <inertia ixx="0.00095806" ixy="0.0" ixz="0.0" iyx="0.0" iyy="0.00095831" iyz="0.0" izx="0.0" izy= "0.0" izz="0.00180386"/>
        </inertial>
    </link>
    <!-- ==================== LINER LINKS AND JOINTS ==================== -->

    <!-- Liner 1 -->
    <link name="liner1">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner1_pbs" type="fixed">
    <parent link="peg_board"/>
    <child link="liner1"/>
    <origin xyz="-0.375 -0.02225 0.176" rpy="0 0 0"/>
    </joint>


    <!-- Liner 2 -->
    <link name="liner2">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner2_pbs" type="fixed">
    <parent link="starter_gear"/>
    <child link="liner2"/>
    <origin xyz="0.0 -0.028 0.051" rpy="0 0 0"/>
    </joint>


    <!-- Liner 3 -->
    <link name="liner3">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner3_pbf" type="fixed">
    <parent link="peg_board"/>
    <child link="liner3"/>
    <origin xyz="0.0 -0.02225 0.176" rpy="0 0 0"/>
    </joint>


    <!-- Liner 4 -->
    <link name="liner4">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner4_pbf" type="fixed">
    <parent link="first_gear"/>
    <child link="liner4"/>
    <origin xyz="0.0 -0.028 0.051" rpy="0 0 0"/>
    </joint>


    <!-- Liner 5 -->
    <link name="liner5">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner5_pbf" type="fixed">
    <parent link="peg_board"/>
    <child link="liner5"/>
    <origin xyz="0.25 -0.02225 0.176" rpy="0 0 0"/>
    </joint>


    <!-- Liner 6 -->
    <link name="liner6">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner6_pb2" type="fixed">
    <parent link="second_gear"/>
    <child link="liner6"/>
    <origin xyz="0.0 -0.028 0.051" rpy="0 0 0"/>
    </joint>


    <!-- Liner 7 -->
    <link name="liner7">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner7_pbt" type="fixed">
    <parent link="peg_board"/>
    <child link="liner7"/>
    <origin xyz="0.45 -0.02225 0.176" rpy="0 0 0"/>
    </joint>


    <!-- Liner 8 -->
    <link name="liner8">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
        <box size="0.005 0.005 0.001"/>
        </geometry>
        <material name="Red">
        <color rgba="1.0 0.0 0.0 1.0"/>
        </material>
    </visual>
    </link>

    <joint name="liner8_pbt" type="fixed">
    <parent link="third_gear"/>
    <child link="liner8"/>
    <origin xyz="0.0 -0.028 0.051" rpy="0 0 0"/>
    </joint>

